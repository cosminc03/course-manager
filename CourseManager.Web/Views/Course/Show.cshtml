<p>Title: @ViewBag.Course.Title</p>

@if (@ViewBag.IsStudent) { 
    <p>Subscribed: @if(@ViewBag.IsSubscribed) { <span>Yes</span> } else { <span>No</span> } </p>
}

<p>Year: @ViewBag.Course.Year</p>
<p>Semester: @ViewBag.Course.Semester</p>
<p>Description: @ViewBag.Course.Description</p>
<p>Owner: @ViewBag.Course.Owner.FirstName - @ViewBag.Course.Owner.LastName</p>
<p>Associates:</p>

<ul>
    @foreach (var assoc in @ViewBag.Associates)
    {
        <li>@assoc.FirstName  @assoc.LastName</li>
    }
</ul>