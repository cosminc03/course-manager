@model CourseCreateViewModel

@{
    ViewBag.Title = "Delete";
}
<div class="pm-body clearfix">
    <h2 class="p-l-25 p-t-20 inline-block">Are you sure you want to delete course "@Model.Title" ?</h2>
    <form asp-controller="Course" asp-action="DeleteConfirmed" asp-route-returnurl="@ViewData["ReturnUrl"]" method="post" class="form-horizontal" role="form" novalidate>
        <button type="submit" class="btn btn-default">Delete</button>
    </form>
    <div class="card-body card-padding">
    </div>
</div>

